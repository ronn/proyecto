{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "semi": 0,
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "comma-dangle": 0,
    "space-before-blocks": 0,
    "eol-last": 0,
    "react/prop-types": 0,
    "keyword-spacing": 0,
    "padded-blocks": 0,
    "curly": 0
  },
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "env": {
    "browser": true,
    "node": true
  }
}